<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>ABChwat</servlet-name>
		<servlet-class>com.myorg.javacourse.ABChwatServlet</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>ABChwat</servlet-name>
		<url-pattern>/abchwat</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>MyNewServlet</servlet-name>
		<servlet-class>com.myorg.javacourse.MyNewServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>MyNewServlet</servlet-name>
		<url-pattern>/mynewservlet</url-pattern>
	</servlet-mapping>
	
		<servlet>
		<servlet-name>StockServlet</servlet-name>
